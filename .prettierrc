{
  "plugins": ["prettier-plugin-tailwindcss"],
    "printWidth": 200,       
    "tabWidth": 2,           
    "useTabs": false,         
    "semi": true,             
    "singleQuote": false,     
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,  
    "trailingComma": "none",  
    "bracketSpacing": true,   
    "jsxBracketSameLine": false, 
    "arrowParens": "always",  
    "endOfLine": "auto"  
  
}